<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pk.reviewcollector">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".new_feeds"
            android:label="@string/title_activity_new_feeds"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Edit_Profile" />
        <activity android:name=".Login" />
        <activity android:name=".Create_Account" />
        <activity android:name=".Email_Varification" />
        <activity android:name=".Create_Poll" />
        <activity android:name=".GroupsArea" />
        <activity android:name=".ReviewPoll" />
        <activity android:name=".ViewDelayedGroups" />
        <activity android:name=".ViewJoinGroups" />
        <activity android:name=".ReviewApp" />
        <activity android:name=".ViewReviewAdmin" />
        <activity android:name=".Reports" />
        <activity android:name=".ViewReportsListActivity" />
        <activity android:name=".AddMembers" />
        <activity android:name=".ViewPendingGroupMembers" />
        <activity android:name=".ViewProfile" />
        <activity
            android:name=".GroupAdminUser"
            android:label="@string/title_activity_group_admin_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ViewReportsByUserJoin"></activity>
    </application>

</manifest>